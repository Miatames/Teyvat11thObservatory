<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.genshintool.dao.GobletDao">

    <resultMap id="GobletMapper" type="com.genshintool.pojo.GobletPojo">
        <id property="timeGoblet" column="timeGoblet"></id>
        <result property="elemGoblet" column="elemGoblet"></result>
        <result property="physGoblet" column="physGoblet"></result>
        <result property="atkGoblet" column="atkGoblet"></result>
        <result property="defGoblet" column="defGoblet"></result>
        <result property="hpGoblet" column="hpGoblet"></result>
        <result property="emGoblet" column="emGoblet"></result>
    </resultMap>

    <select id="doGobletSubmitDao" resultType="gobletpojo">
        insert into GOBLET(ELEMGOBLET,PHYSGOBLET,ATKGOBLET,DEFGOBLET,HPGOBLET,EMGOBLET,TIMEGOBLET)
        values(#{elemGoblet},#{physGoblet},#{atkGoblet},#{defGoblet},#{hpGoblet},#{emGoblet},#{timeGoblet})
    </select>

</mapper>